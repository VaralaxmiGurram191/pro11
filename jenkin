pipeline {
    agent any  

    stages {

stage('clone') { 
            steps {
                git 'https://github.com/VaralaxmiGurram191/pro11.git'
            }
        }
                
stage('Build') {
            steps {
                sh 'javac Helloworld.java'
            }
        }
stage('Test') {
steps{
sh'java helloworld'
}
}
}
}
